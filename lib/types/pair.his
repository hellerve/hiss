(load ".stdlib")
(type:pair {first second})

(fun {make_pair h t}{
     (const pair {h t})})

(fun {pair? c}{
     (if (== (type? c) "pair"))
        {true}
        {false}})

(fun {car c}{
     (if (pair? c))
        {from c first}
        {error "Argument is not a pair"}})

{fun {cdr c}{
    (if (pair? c))
        {from c second}
        {error "Argument is not a pair"}})
